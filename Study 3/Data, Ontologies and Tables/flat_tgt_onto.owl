<?xml version="1.0" encoding="utf-8"?>
<rdf:RDF
  xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#"
  xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
  xmlns:owl="http://www.w3.org/2002/07/owl#"
>
  <owl:DatatypeProperty rdf:about="http://example.org/tgt#person_id">
    <rdfs:domain rdf:resource="http://example.org/tgt#Measurement"/>
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#integer"/>
    <rdfs:label xml:lang="en">person id</rdfs:label>
    <rdfs:comment xml:lang="en">Person ID linked to the measurement.</rdfs:comment>
  </owl:DatatypeProperty>
  <owl:DatatypeProperty rdf:about="http://example.org/tgt#measurement_datetime">
    <rdfs:domain rdf:resource="http://example.org/tgt#Measurement"/>
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#dateTime"/>
    <rdfs:label xml:lang="en">measurement datetime</rdfs:label>
    <rdfs:comment xml:lang="en">Full datetime of the measurement.</rdfs:comment>
  </owl:DatatypeProperty>
  <owl:DatatypeProperty rdf:about="http://example.org/tgt#value_as_concept_id">
    <rdfs:domain rdf:resource="http://example.org/tgt#Measurement"/>
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#integer"/>
    <rdfs:label xml:lang="en">value as concept id</rdfs:label>
    <rdfs:comment xml:lang="en">Concept ID for categorical value.</rdfs:comment>
  </owl:DatatypeProperty>
  <owl:DatatypeProperty rdf:about="http://example.org/tgt#range_low">
    <rdfs:domain rdf:resource="http://example.org/tgt#Measurement"/>
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#decimal"/>
    <rdfs:label xml:lang="en">range low</rdfs:label>
    <rdfs:comment xml:lang="en">Lower limit of reference range.</rdfs:comment>
  </owl:DatatypeProperty>
  <owl:DatatypeProperty rdf:about="http://example.org/tgt#measurement_type_concept_id">
    <rdfs:domain rdf:resource="http://example.org/tgt#Measurement"/>
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#integer"/>
    <rdfs:label xml:lang="en">measurement type concept id</rdfs:label>
    <rdfs:comment xml:lang="en">Type of measurement (e.g., EHR-derived).</rdfs:comment>
  </owl:DatatypeProperty>
  <owl:DatatypeProperty rdf:about="http://example.org/tgt#measurement_id">
    <rdfs:domain rdf:resource="http://example.org/tgt#Measurement"/>
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#integer"/>
    <rdfs:label xml:lang="en">measurement id</rdfs:label>
    <rdfs:comment xml:lang="en">Unique identifier for the measurement.</rdfs:comment>
  </owl:DatatypeProperty>
  <owl:DatatypeProperty rdf:about="http://example.org/tgt#measurement_source_concept_id">
    <rdfs:domain rdf:resource="http://example.org/tgt#Measurement"/>
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#integer"/>
    <rdfs:label xml:lang="en">measurement source concept id</rdfs:label>
    <rdfs:comment xml:lang="en">Original source concept ID (e.g., NPU code).</rdfs:comment>
  </owl:DatatypeProperty>
  <owl:Ontology rdf:about="http://example.org/tgt"/>
  <owl:DatatypeProperty rdf:about="http://example.org/tgt#range_high">
    <rdfs:domain rdf:resource="http://example.org/tgt#Measurement"/>
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#decimal"/>
    <rdfs:label xml:lang="en">range high</rdfs:label>
    <rdfs:comment xml:lang="en">Upper limit of reference range.</rdfs:comment>
  </owl:DatatypeProperty>
  <owl:DatatypeProperty rdf:about="http://example.org/tgt#measurement_source_value">
    <rdfs:domain rdf:resource="http://example.org/tgt#Measurement"/>
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
    <rdfs:label xml:lang="en">measurement source value</rdfs:label>
    <rdfs:comment xml:lang="en">Original value as provided.</rdfs:comment>
  </owl:DatatypeProperty>
  <owl:DatatypeProperty rdf:about="http://example.org/tgt#measurement_concept_id">
    <rdfs:domain rdf:resource="http://example.org/tgt#Measurement"/>
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#integer"/>
    <rdfs:label xml:lang="en">measurement concept id</rdfs:label>
    <rdfs:comment xml:lang="en">Mapped OMOP concept ID.</rdfs:comment>
  </owl:DatatypeProperty>
  <owl:DatatypeProperty rdf:about="http://example.org/tgt#unit_source_value">
    <rdfs:domain rdf:resource="http://example.org/tgt#Measurement"/>
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
    <rdfs:label xml:lang="en">unit source value</rdfs:label>
    <rdfs:comment xml:lang="en">Original textual unit from source.</rdfs:comment>
  </owl:DatatypeProperty>
  <owl:DatatypeProperty rdf:about="http://example.org/tgt#unit_concept_id">
    <rdfs:domain rdf:resource="http://example.org/tgt#Measurement"/>
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#integer"/>
    <rdfs:label xml:lang="en">unit concept id</rdfs:label>
    <rdfs:comment xml:lang="en">Mapped unit concept ID.</rdfs:comment>
  </owl:DatatypeProperty>
  <owl:DatatypeProperty rdf:about="http://example.org/tgt#measurement_time">
    <rdfs:domain rdf:resource="http://example.org/tgt#Measurement"/>
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#time"/>
    <rdfs:label xml:lang="en">measurement time</rdfs:label>
    <rdfs:comment xml:lang="en">Time the measurement was taken.</rdfs:comment>
  </owl:DatatypeProperty>
  <owl:DatatypeProperty rdf:about="http://example.org/tgt#measurement_date">
    <rdfs:domain rdf:resource="http://example.org/tgt#Measurement"/>
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#date"/>
    <rdfs:label xml:lang="en">measurement date</rdfs:label>
    <rdfs:comment xml:lang="en">Date the measurement was taken.</rdfs:comment>
  </owl:DatatypeProperty>
  <owl:DatatypeProperty rdf:about="http://example.org/tgt#operator_concept_id">
    <rdfs:domain rdf:resource="http://example.org/tgt#Measurement"/>
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#integer"/>
    <rdfs:label xml:lang="en">operator concept id</rdfs:label>
    <rdfs:comment xml:lang="en">Operator concept (e.g., 4171756 for '&lt;', 4172704 for '&gt;') from OMOP vocabulary.</rdfs:comment>
  </owl:DatatypeProperty>
  <owl:DatatypeProperty rdf:about="http://example.org/tgt#provider_id">
    <rdfs:domain rdf:resource="http://example.org/tgt#Measurement"/>
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#integer"/>
    <rdfs:label xml:lang="en">provider id</rdfs:label>
    <rdfs:comment xml:lang="en">Provider who conducted the measurement.</rdfs:comment>
  </owl:DatatypeProperty>
  <owl:DatatypeProperty rdf:about="http://example.org/tgt#value_as_number">
    <rdfs:domain rdf:resource="http://example.org/tgt#Measurement"/>
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#decimal"/>
    <rdfs:label xml:lang="en">value as number</rdfs:label>
    <rdfs:comment xml:lang="en">Numeric value of the measurement.</rdfs:comment>
  </owl:DatatypeProperty>
  <owl:Class rdf:about="http://example.org/tgt#Measurement">
    <rdfs:label xml:lang="en">Measurement</rdfs:label>
  </owl:Class>
</rdf:RDF>
